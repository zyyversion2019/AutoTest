<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="logincase">
  <!--获取登陆接口case-->
  <select id="selectByCaseName" parameterType="String" resultType="com.zyy.testauto.model.LoginCase">
        select * from login_case
        where casename=#{casename};
    </select>
</mapper>
